{
  "name": "react-weather-app",
  "version": "0.1.0",
  "description": "Simple weather tracker created with react, typescript and webpack.",
  "keywords": [
    "Weather",
    "Tracker"
  ],
  "author": "Kary Ma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karyma101/react-weather-app.git"
  },
  "bugs": {
    "url": "https://github.com/karyma101/react-weather-app/issues"
  },
  "homepage": "https://github.com/karyma101/react-weather-app",
  "scripts": {
    "build": "npm run clean-dist && webpack --config=webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "dev": "webpack serve --config=webpack/dev.js",
    "prod": "npm run build && node express.js",
    "test": "jest --coverage --watchAll --config=jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "image-webpack-loader": "^7.0.1",
    "jest": "^27.0.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0"
  }
